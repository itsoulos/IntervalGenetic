\series default
 
\begin_inset Formula $a_{m}$
\end_inset

=
\begin_inset Formula $-F\times c_{ij}$
\end_inset

, 
\begin_inset Formula $b_{m}$
\end_inset

=
\begin_inset Formula $F\times c_{ij}$
\end_inset


\end_layout

\begin_layout Enumerate

