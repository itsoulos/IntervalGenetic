\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Enumerate

\series bold
Set
\series default
 m=0
\end_layout

\begin_layout Enumerate

\series bold
Set
\series default
 
\begin_inset Formula $F>1,\ B>0$
\end_inset


\end_layout

\begin_layout Enumerate

\series bold
For
\series default
 
\begin_inset Formula $i=1..k$
\end_inset

 
\series bold
do
\end_layout

\begin_deeper
\begin_layout Enumerate

\series bold
For
\series default
 
\begin_inset Formula $j=1..d$
\end_inset

 
\series bold
