\begin_inset Flex URL
status collapsed

\begin_layout Plain Layout

https://archive.ics.uci.edu/ml/index.php
\end_layout

\end_inset


\begin_inset CommandInset citation
LatexCommand citep
key "UCL"
literal "false"

\end_inset


\end_layout

\begin_layout Enumerate
Keel repository, 
\begin_inset Flex URL
status collapsed

\begin_layout Plain Layout

https://sci2s.ugr.es/keel/datasets.php
\end_layout

\end_inset


\begin_inset CommandInset citation
LatexCommand cite
key "Keel"
literal "false"

\end_inset

.
\end_layout

\begin_layout Enumerate
The Statlib URL 
\begin_inset Flex URL
status collapsed

\begin_layout Plain Layout

ftp://lib.stat.cmu.edu/datasets/index.html 
\end_layout

\end_inset

.
\end_layout

