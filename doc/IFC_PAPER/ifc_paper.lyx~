\begin_layout LyX-Code
           | <digit><digit><digit>     (2)
\end_layout

\begin_layout LyX-Code
<digit>  ::= 0 (0)             
\end_layout

\begin_layout LyX-Code
           | 1 (1)             
\end_layout

\begin_layout LyX-Code
           | 2 (2)             
\end_layout

\begin_layout LyX-Code
           | 3 (3)             
\end_layout

\begin_layout LyX-Code
           | 4 (4)             
\end_layout

\begin_layout LyX-Code
           | 5 (5)             
\end_layout

\begin_layout LyX-Code
           | 6 (6)             
\end_layout

\begin_layout LyX-Code
           | 7 (7)             
\end_layout

\begin_layout LyX-Code
           | 8 (8)             
\end_layout

